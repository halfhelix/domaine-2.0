---
import Hero from './hero/Hero.astro';
import Clients from './clients/Clients.astro';
import About from './about/About.astro';
import Services from './services/Services.astro';
import Agencies from './agencies/Agencies.astro';
import Partners from './partners/Partners.astro';
import Contact from './contact/Contact';
import TextHero from './hero/TextHero.astro'
import MediaFullbleed from './media/MediaFullbleed.astro'
import MediaGrid from './media/MediaGrid.astro'
import ServiceDetail from './services/ServiceDetail.astro'
import TextColumns from './text/TextColumns.astro'
import ContentBlocks from './global/ContentBlocks.astro'
import MediaGallery from './global/MediaGallery.astro'
import TextImage from './global/TextImage.astro'
import EventSpeakers from './event/EventSpeakers.astro';
import Form from './global/Form.astro';
import RichText from './global/RichText.astro';
import Work from './global/Work.astro';

interface Props {
	block: any;
}
const { block } = Astro.props;

// console.log(block)

---

{block._type == 'sectionHero' && <Hero content={block} />}
{block._type == 'sectionClients' && <Clients content={block} />}
{block._type == 'sectionAbout' && <About content={block} />}
{block._type == 'sectionServices' && <Services content={block} />}
{block._type == 'sectionContact' && <Contact client:only="solid-js" content={block} />}
{block._type == 'sectionWork' && <Work content={block} />}
{block._type == 'sectionPartners' && <Partners content={block} />}
{block._type == 'sectionAgencies' && <Agencies content={block} />}
{block._type == 'sectionTextHero' && <TextHero content={block} />}
{block._type == 'sectionServiceDetail' && <ServiceDetail content={block} />}
{block._type == 'sectionMediaFullbleed' && <MediaFullbleed content={block} />}
{block._type == 'sectionMediaGrid' && <MediaGrid content={block} />}
{block._type == 'sectionTextColumns' && <TextColumns content={block} />}

<!-- Event Sections -->
{block._type == 'sectionSpeakers' && <EventSpeakers content={block} />}

<!-- Global Blocks -->
{block._type == 'sectionMediaGallery' && <MediaGallery content={block} />}
{block._type == 'sectionContentBlocks' && <ContentBlocks content={block} />}
{block._type == 'sectionTextImage' && <TextImage content={block} />}
{block._type == 'sectionContactForm' && <Form content={block} />}
{block._type == 'sectionRichText' && <RichText content={block} />}