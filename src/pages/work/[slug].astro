---
import { getProjects } from '../../utils/sanityClient';


export async function getStaticPaths() {

const projects = await getProjects();

return projects.map((project) => {
  return {
    params: { slug: project.slug.current },
    props: { data: project }
  }
})
}

const { slug } = Astro.params;
---
<h1>{slug}</h1>